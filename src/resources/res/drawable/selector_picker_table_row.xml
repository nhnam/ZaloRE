<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ffff9900" android:endColor="#ffffcc33" android:angle="90.0" android:centerColor="#ffffcc00" />
            <stroke android:width="2.0dip" color="#ffffffff" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <gradient android:startColor="#ffff9900" android:endColor="#ffff9900" android:angle="90.0" android:centerColor="#ffff9900" />
            <stroke android:width="2.0dip" color="#ffffffff" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ffeeeeee" android:endColor="#ffeeeeee" android:angle="90.0" />
            <stroke android:width="2.0dip" color="#ffffffff" />
        </shape>
    </item>
</selector>